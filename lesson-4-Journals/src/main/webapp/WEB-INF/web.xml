<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>registration</servlet-name>
		<display-name>registration</display-name>
		<description></description>
		<servlet-class>ua.lviv.lgs.RegistrationServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<display-name>LoginServlet</display-name>
		<description></description>
		<servlet-class>ua.lviv.lgs.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>VisitorServlet</servlet-name>
		<display-name>VisitorServlet</display-name>
		<description></description>
		<servlet-class>ua.lviv.lgs.VisitorServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>JournalManager</servlet-name>
		<display-name>JournalManager</display-name>
		<description></description>
		<servlet-class>ua.lviv.lgs.JournalManager</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>registration</servlet-name>
		<url-pattern>/registration</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>VisitorServlet</servlet-name>
		<url-pattern>/VisitorServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>JournalManager</servlet-name>
		<url-pattern>/JournalManager</url-pattern>
	</servlet-mapping>
	
</web-app>
